{
    
    "name": "WebPage Summarizer With UI",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "Summarizes the text from your webpage.",
    "permissions": ["activeTab", "contentSettings", "tabs"],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["https://api.com/*"],
    "icons": {
        "1": "assets/summarEaselogo.png"
    }

}
